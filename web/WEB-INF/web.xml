        <servlet-name>ServiceClientOrderChangeStatus</servlet-name>
        <url-pattern>/ServiceClientOrderChangeStatus</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProductDetailClientOrderServiceDelete</servlet-name>
        <servlet-class>service.delete.form.ProductDetailClientOrderService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProductDetailClientOrderServiceDelete</servlet-name>
        <url-pattern>/ProductDetailClientOrderServiceDelete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProductDetailClientOrderServiceInsert</servlet-name>
        <servlet-class>service.insert.form.ProductDetailClientOrderService</servlet-class>
    </servlet>
