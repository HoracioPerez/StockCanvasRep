<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>LoadArchivo</servlet-name>
        <servlet-class>htmlTable.LoadArchivo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientServiceInsert</servlet-name>
        <servlet-class>service.insert.entity.ClientService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClientServiceInsert</servlet-name>
        <url-pattern>/ClientServiceInsert</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ClientOrderServiceInsert</servlet-name>
        <servlet-class>service.insert.form.ClientOrderService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClientOrderServiceInsert</servlet-name>
        <url-pattern>/ClientOrderServiceInsert</url-pattern>
    </servlet-mapping>
    <servlet>
