        <servlet-name>ClientOrderTotalService</servlet-name>
        <url-pattern>/ClientOrderTotalService</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProductServiceInsert</servlet-name>
        <servlet-class>service.insert.entity.ProductService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProductServiceInsert</servlet-name>
        <url-pattern>/ProductServiceInsert</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DeliveryNoteServiceInsert</servlet-name>
        <servlet-class>service.insert.form.DeliveryNoteService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeliveryNoteServiceInsert</servlet-name>
        <url-pattern>/DeliveryNoteServiceInsert</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>-1</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
